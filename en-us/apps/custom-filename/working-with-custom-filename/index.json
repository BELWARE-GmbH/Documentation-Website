        "title" : "Scenarios for Custom Filename",
        "date_published" : "2023-09-19T00:00:00Z",
        "date_modified" : "2023-09-19T00:00:00Z",
        "id" : "https://docs.belware.de/en-us/apps/custom-filename/working-with-custom-filename/scenarios/",
